    <line rend="indent3">that which thou surveyest</line>
    <line>From thine unenvied throne</line>
    <anchor xml:id="e1-0041.01"/>
    <line>[SKETCH IN PENCIL OF TREE]</line>
    <line><add><metamark function="displacement">&#60;</metamark></add></line>
    <line><add next="#e1-0041.03" corresp="#e1-0040.08">Have it's deaf waves not heard my
        agony?</add></line>
    <addSpan spanTo="#e1-0041.02" corresp="#e1-0040.07"/>
    <line rend="indent1">Ah me, alas, pain, pain ever forever!</line>
    <line rend="indent1">No change, no pause, <mod>
        <del rend="overwritten">and</del>
        <add place="intralinear">no</add>
      </mod> hope!--yet I endure</line>
    <anchor xml:id="e1-0041.02"/>
    <line><add xml:id="e1-0041.03" corresp="#e1-0040.09">Ah me alas pain ever forever</add></line>
    <line><add corresp="#e1-0040.10">Mocking me</add></line>
